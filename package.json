  "license": "MIT",
  "scripts": {
    "build": "npm run build:packages && nx run-many -t build -p @cyl-app/finance-cyl-api",
    "build:finance-cyl-api": "npm run build:packages && nx build @cyl-app/finance-cyl-api -skip-nx-cache",
    "build:packages": "nx run-many -t build -p @cyl-app/dto --skip-nx-cache",
    "dev": "npm run build:packages && nx run-many -t dev -p @cyl-app/finance-cyl-api --output-style=stream",
    "dev:finance-cyl-api": "npm run build:packages && nx dev @cyl-app/finance-cyl-api --output-style=stream",
    "lint": "npm run lint:packages && npm run lint:finance-cyl-api",
    "lint:finance-cyl-api": "nx lint @cyl-app/finance-cyl-api",
    "format": "npm run format:packages && npm run format:finance-cyl-api",
    "format:finance-cyl-api": "prettier --write apps/finance-cyl-api"
  },
  "private": true,
  "devDependencies": {
