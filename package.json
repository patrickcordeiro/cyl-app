    "@swc/helpers": "~0.5.17",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "2.8.19",
    "@types/express": "5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "24.5.2",
    "autoprefixer": "10.4.21",
    "esbuild": "^0.25.12",
    "eslint": "9.36.0",
    "eslint-config-next": "15.5.4",
    "eslint-config-prettier": "10.1.8",
