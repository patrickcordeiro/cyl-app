{
  "name": "@cyl-app/finance-cyl-api",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "targets": {
      "prune-lockfile": {
        "dependsOn": [
          "build"
        ],
        "cache": true,
        "executor": "@nx/js:prune-lockfile",
        "outputs": [
          "{workspaceRoot}/apps/finance-cyl-api/dist/package.json",
          "{workspaceRoot}/apps/finance-cyl-api/dist/package-lock.json"
        ],
        "options": {
          "buildTarget": "build"
        }
      },
      "copy-workspace-modules": {
        "dependsOn": [
          "build"
        ],
        "cache": true,
        "outputs": [
          "{workspaceRoot}/apps/finance-cyl-api/dist/workspace_modules"
        ],
        "executor": "@nx/js:copy-workspace-modules",
        "options": {
          "buildTarget": "build"
        }
      },
      "prune": {
        "dependsOn": [
          "prune-lockfile",
          "copy-workspace-modules"
        ],
        "executor": "nx:noop"
      },
      "dev": {
        "continuous": true,
        "executor": "@nx/js:node",
        "defaultConfiguration": "development",
        "dependsOn": [
          "build"
        ],
        "options": {
          "buildTarget": "@cyl-app/finance-cyl-api:build",
          "runBuildTargetDependencies": false
        },
        "configurations": {
          "development": {
            "buildTarget": "@cyl-app/finance-cyl-api:build:development"
          },
          "production": {
            "buildTarget": "@cyl-app/finance-cyl-api:build:production"
          }
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint ./src --ext .ts",
    "format": "prettier --write ./src",
    "typeorm": "typeorm-ts-node-esm",
    "migrations:create": "typeorm migration:create",
    "migrations:run": "typeorm migration:run --",
    "migrations:show": "typeorm migration:show --"
  },
  "dependencies": {
    "@cyl-app/dto": "*",
    "express": "^4.21.2"
  }
}
